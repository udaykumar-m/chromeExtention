{
  "manifest_version": 3,
  "name": "AI pal",
  "version": "1.0",
  "description": "AI extenstion that helps you laugh, generate captions, write tweets and more..",
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "icons": {
    "16": "img/icon16.png"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "./js/backgroundTasks.js"
  },
  "version_name": "1.0.5"
}
