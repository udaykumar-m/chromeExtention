{
  "manifest_version": 3,
  "name": "Funny AI",
  "version": "1.0",
  "description": "AI extenstion that helps you laugh, generate captions, write tweets and more..",
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "./js/backgroundTasks.js"
  },
  "version_name": "1.0.5"
}
